<% if @housings && @housings.count > 0 %>
    $("#tbl_housings").html("<%= escape_javascript(render partial: 'results', :locals => { :places => @housings }) %>");
<% else %>
    $("#tbl_housings").html("<%= escape_javascript(render partial: 'no_results') %>");
<% end %>


<% if @adverts && @adverts.count > 0 %>
    $("#tbl_adverts").html("<%= escape_javascript(render partial: 'results_advert', :locals => { :places => @adverts }) %>");
<% else %>
    $("#tbl_adverts").html("<%= escape_javascript(render partial: 'no_results') %>");
<% end %>


$("#spinner").removeClass('spinner').fadeIn(100);

