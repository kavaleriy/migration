<table class="table">
<thead>
<tr>
  <th><%= t('.area') %></th>
  <th><%= t('.region') %></th>
  <th><%= t('.house') %></th>
  <th><%= t('.places') %></th>
  <th><%= t('.school') %></th>
  <th><%= t('.kgarten') %></th>
</tr>
</thead>
<tbody>
<% @housings.each do |housing| %>
    <% coatuu = Koatuu.coatuu_to_json(housing.koatuu_code) %>
    <% if coatuu %>
    <tr>
      <td><%= coatuu[:area] %> </td>
      <td><%= coatuu[:name] || coatuu[:region] %> </td>
      <td><%= housing.house.name %></td>
      <td>
        <a href="<%= static_centers_path %>#tel_0510100000" class=""><%= housing.qty_places %></a> /
        <a href="<%= static_centers_path %>#tel_0510100000" class=""><%= work_class(housing.koatuu_code) %></a>
      </td>
      <td ><i style="padding-left: 25px;" class="<%= school_class(housing.has_school) %>"></i></td>
      <td><i style="padding-left: 25px;" class="<%= kgarten_class(housing.has_kgarden) %>"></i></td>
      <td>
       </td>
    </tr>
    <% end %>
<% end %>
</tbody>
</table>

<div class="flickr_pagination ">
  <%= paginate @housings, :container => false %>
  <div class="page_info">
    <!--<%= page_entries_info @housings %>-->
  </div>
</div>

