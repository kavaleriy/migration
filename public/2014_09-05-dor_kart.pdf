			return variants[Math.floor(Math.random() * variants.length)];
		}

		function chooseTheme(themeId){
			var themeConfig = patron.themesConfig[themeId],
				themeVariant = 'all',
				hours = (new Date()).getHours();
			if (themeConfig && themeConfig.variants) {
				if (themeConfig.currentVariant) {
					themeVariant = themeConfig.currentVariant;
				} else {
					themeVariant = themeConfig.currentVariant = getRandomThemeVariant(themeConfig.variants);
				}
			} else {
				switch (themeId) {
					case 't1016':
						if (hours >= 6 && hours < 12){
							themeVariant = "morning";
						} else if (hours >= 12 && hours < 18){
							themeVariant = "day";
						} else if (hours >= 18 && hours < 22){
							themeVariant = "evening";
						} else {
							themeVariant = "night";
						}
					break;

					case 't1032':
					case 't1060':
						if (hours >= 7 && hours < 19){
							themeVariant = "day";
						} else {
							themeVariant = "night";
						}
					break;
				}
			}
			return themeVariant;
		}

